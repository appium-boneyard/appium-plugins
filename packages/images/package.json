{
  "name": "@appium/images-plugin",
  "version": "1.0.0",
  "description": "Plugin for working with images and image elements in Appium",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appium/appium-plugins.git"
  },
  "appium": {
    "pluginName": "images",
    "mainClass": "ImageElementPlugin"
  },
  "keywords": [
    "appium",
    "opencv",
    "automation",
    "image",
    "webdriver"
  ],
  "author": "Appium <maintainers@appium.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/appium/appium-plugins/issues"
  },
  "homepage": "https://github.com/appium/appium-plugins#readme",
  "dependencies": {
    "@appium/base-plugin": "^1.5.0",
    "appium-base-driver": "^8.0.0-beta.0",
    "lru-cache": "^6.0.0"
  },
  "scripts": {
    "test:unit": "mocha ./build/test/unit/*-specs.js",
    "test:e2e": "mocha -t 160000 ./build/test/e2e/*-specs.js",
    "copyfiles": "copyfiles ./test/fixtures/*.png ./build"
  },
  "devDependencies": {}
}
